{
	"name": "motionwindui",
	"version": "1.0.0",
	"description": "",
	"private": true,
	"author": "Kyle Gagnon",
	"license": "ISC",
	"type": "module",
	"repository": {
		"url": "https://github.com/MotionWindUI/MotionWindUI"
	},
	"scripts": {
		"build": "turbo build --filter=!@motionwindui/storybook",
		"build:fast": "turbo build:fast --filter=!@motionwindui/storybook",
		"dev": "turbo dev --filter=!@motionwindui/storybook",
		"test": "jest --config ./jest.config.ts",
		"lint": "pnpm lint:pkg",
		"lint:pkg": "eslint -c .eslintrc.json ./packages/**/*.{ts,tsx}",
		"type-check": "turbo type-check --filter=!@motionwindui/storybook",
		"sb": "pnpm --filter @motionwindui/storybook dev",
		"build:sb": "pnpm --filter @motionwindui/storybook build",
		"deploy:sb": "echo 'Deploy StoryBook to GitHub Pages'",
		"test:sb": "pnpm --filter @motionwindui/storybook test:sb",
		"format:check": "prettier --check packages/**/**/src --cache",
		"format:write": "prettier --write packages/**/**/src --cache",
		"turbo:clean": "turbo clean && rm -rf ./node_modules/.cache/turbo",
		"turbo:graph": "pnpm build --graph=dependency-graph.png",
		"clean": "pnpm clean:jest && pnpm clean:dist && pnpm clean:lock && pnpm clean:node-modules && pnpm install --hoist",
		"clean:dist": "rimraf ./packages/**/**/dist",
		"clean:node-modules": "rimraf ./packages/**/**/node_modules && rimraf ./node_modules",
		"clean:lock": "rimraf ./pnpm-lock.yaml",
		"clean:jest": "jest --clearCache",
		"create:component": "plop component",
		"prepare": "husky",
		"validate-branch": "chmod +x ./hooks/branch_name_validation.sh && chmod 755 ./hooks/branch_name_validation.sh && ./hooks/branch_name_validation.sh",
		"release": "changeset publish"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.7",
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@eslint/js": "^9.7.0",
		"@storybook/react": "^8.2.2",
		"@swc-node/jest": "^1.8.12",
		"@swc/core": "^1.7.2",
		"@swc/jest": "^0.2.36",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.4.8",
		"@testing-library/react": "^16.0.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.14.12",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"chromatic": "^11.5.6",
		"concurrently": "^8.2.2",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-react": "^7.34.3",
		"extend-expect": "link:@testing-library/jest-dom/extend-expect",
		"globals": "^15.8.0",
		"husky": "^9.0.11",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"plop": "^4.0.1",
		"prettier": "^3.3.2",
		"prettier-eslint": "^16.3.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"rimraf": "^6.0.1",
		"ts-node": "^10.9.2",
		"tsup": "^8.1.0",
		"turbo": "^2.0.6",
		"typescript": "^5.5.3",
		"typescript-eslint": "^7.16.0"
	},
	"packageManager": "pnpm@9.5.0"
}
