{
	"name": "motionwindui",
	"version": "1.0.0",
	"description": "",
	"private": true,
	"author": "Kyle Gagnon",
	"license": "ISC",
    "type": "module",
	"scripts": {
		"build": "turbo build --filter=!@motionwindui/storybook",
		"build:fast": "turbo build:fast --filter=!@motionwindui/storybook",
		"dev": "turbo dev --filter=!@motionwindui/storybook",
		"test": "turbo test --filter=!@motionwindui/storybook",
		"lint": "pnpm lint:pkg",
		"lint:pkg": "eslint -c .eslintrc.json ./packages/**/*.{ts,tsx}",
		"type-check": "turbo type-check --filter=!@motionwindui/storybook",
		"sb": "pnpm --filter @motionwindui/storybook dev",
		"build:sb": "pnpm --filter @motionwindui/storybook build",
		"deploy:sb": "echo 'Deploy StoryBook to GitHub Pages'",
		"format:check": "prettier --check packages/**/**/src --cache",
		"format:write": "prettier --write packages/**/**/src --cache",
		"turbo:clean": "turbo clean && rm -rf ./node_modules/.cache/turbo",
		"turbo:graph": "pnpm build --graph=dependency-graph.png",
		"prepare": "husky"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@eslint/js": "^9.7.0",
		"@storybook/react": "^8.2.2",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.14.10",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"concurrently": "^8.2.2",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-react": "^7.34.3",
		"globals": "^15.8.0",
		"husky": "^9.0.11",
		"jest": "^29.7.0",
		"prettier": "^3.3.2",
		"prettier-eslint": "^16.3.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tsup": "^8.1.0",
		"turbo": "^2.0.6",
		"typescript": "^5.5.3",
		"typescript-eslint": "^7.16.0"
	},
	"packageManager": "pnpm@9.5.0"
}
