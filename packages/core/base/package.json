{
  "name": "@motionwindui/base",
  "version": "1.0.0",
  "description": "The base package for MotionWindUI",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build:fast --watch",
    "clean": "rimraf dist .turbo",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "base"
  ],
  "author": "Kyle Gagnon",
  "license": "ISC",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  }
}
