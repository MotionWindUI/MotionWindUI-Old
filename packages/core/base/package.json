{
  "name": "@motionwindui/base",
  "version": "1.0.1",
  "description": "The base package for MotionWindUI",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build --watch",
    "clean": "rimraf dist .turbo",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "base"
  ],
  "author": "Kyle Gagnon",
  "license": "ISC",
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  }
}
